# Entrada

def programa():
    LIMITE = 100
    peso = float(input('Informe o peso total de peixes: '))

    if peso <= LIMITE:
        print('Peso permitido')

    else:
        print('Excesso de peso')
        multa = (peso - LIMITE) * 4
        print(f'Excesso de peso, a multa correspondente Ã© R$ {multa},00')

programa()